ACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA","file":"Details.vue","sourceRoot":"src/views","sourcesContent":["<template>\r\n  <div class=\"bigbox\">\r\n    <div class=\"detail-topbox\" ref=\"topbox\">\r\n      <div class=\"detail-topbox-item\" v-if=\"seen\" @click=\"goclick\">\r\n        <img src=\"../assets/向左圆箭头.png\" />\r\n      </div>\r\n      <div class=\"detail-topbox-item topelse\" v-else @click=\"goclick\">\r\n        <img src=\"../assets/向左.png\" />\r\n      </div>\r\n\r\n      <div class=\"detail-topbox-item1\" v-if=\"isdis\">\r\n        <span\r\n          class=\"topbox-item1-list\"\r\n          ref=\"topboxspan1\"\r\n          :class=\"{active:this.scrolltop>0&&this.scrolltop<=942}\"\r\n          @click=\"clickone\"\r\n        >商品</span>\r\n        <span\r\n          class=\"topbox-item1-list\"\r\n          ref=\"topboxspan2\"\r\n          :class=\"{active:this.scrolltop>942&&this.scrolltop<=1515}\"\r\n          @click=\"clicktwo($refs.span1.offsetTop)\"\r\n        >评价</span>\r\n        <span\r\n          class=\"topbox-item1-list\"\r\n          ref=\"topboxspan3\"\r\n          :class=\"{active:this.scrolltop>1515}\"\r\n          @click=\"clicktwo($refs.span2.offsetTop)\"\r\n        >详情</span>\r\n      </div>\r\n\r\n      <div class=\"detail-topbox-item\" v-if=\"seen\">\r\n        <img src=\"../assets/分享.png\" />\r\n      </div>\r\n      <div class=\"detail-topbox-item topelse\" v-else>\r\n        <img src=\"../assets/分享1.png\" />\r\n      </div>\r\n    </div>\r\n    <div class=\"datail-center-item14 posibox\" v-if=\"seen11\">商品介绍</div>\r\n    <!-- 中间 -->\r\n    <transition>\r\n      <div class=\"detail-centerbox\" @scroll=\"myscroll\" ref=\"scrollbox\">\r\n        <!-- 轮播图 -->\r\n        <div class=\"lunbobox\">\r\n          <swiper ref=\"mySwiper\" :options=\"swiperOptions\" >\r\n            <swiper-slide class=\"box\" v-for=\"item in list[0]\" :key=\"item.item\">\r\n              <img :src=\"item.img\" alt class=\"img11\" />\r\n            </swiper-slide>\r\n          </swiper>\r\n          <div class=\"swiper-pagination\" slot=\"pagination\"></div>\r\n        </div>\r\n\r\n        <div class=\"pricebox\">\r\n          <span>￥</span>\r\n          <span class=\"pricebox-span2\" v-for=\"item in list[1]\" :key=\"item.item\">{{item.price}}</span>\r\n        </div>\r\n        <div class=\"detail-center-item1\">\r\n          <img src=\"../assets/en.jpg\" />\r\n          <span v-for=\"item in list[2]\" :key=\"item.item\">{{item.title}}</span>\r\n        </div>\r\n        <div class=\"detail-center-item2\" v-for=\"item in list3\" :key=\"item.item\" :item=\"item\">\r\n          <div class=\"detail-item2-list\">\r\n            <img :src=\"item.one\" />\r\n            <span>{{item.title1}}</span>\r\n          </div>\r\n          <div class=\"detail-item2-list\">\r\n            <img :src=\"item.two\" />\r\n            <span>{{item.title2}}</span>\r\n          </div>\r\n          <div class=\"detail-item2-list lastimg\">\r\n            <img :src=\"item.three\" />\r\n            <span>{{item.title3}}</span>\r\n          </div>\r\n          <span class=\"detail-center-item3\">{{item.title4}}</span>\r\n          <div class=\"kong\"></div>\r\n          <div class=\"detail-center-item4\">\r\n            <div>已选</div>\r\n            <div class=\"detail-item4-list listt1\">\r\n              <span>{{item.title5}}</span>\r\n              <img src=\"../assets/箭头 right.png\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"detail-center-item4\">\r\n            <div>送至</div>\r\n            <div class=\"detail-item4-list\">\r\n              <div>\r\n                <img src=\"../assets/定位.png\" />\r\n                <span>苏州市 工业园区</span>\r\n                <span style=\"color:#ff6700;margin-left:5px\">有现货</span>\r\n              </div>\r\n              <img src=\"../assets/箭头 right.png\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"detail-center-item5\">\r\n            <div class=\"detail-item5-list1\">\r\n              <div class=\"detail-item5-list1box\">\r\n                <img src=\"../assets/勾.png\" alt />\r\n                <span>小米自营</span>\r\n              </div>\r\n              <div class=\"detail-item5-list1box\">\r\n                <img src=\"../assets/勾.png\" alt />\r\n                <span>小米发货</span>\r\n              </div>\r\n              <div class=\"detail-item5-list1box\">\r\n                <img src=\"../assets/勾.png\" alt />\r\n                <span>7天无理由退货</span>\r\n              </div>\r\n            </div>\r\n            <div class=\"detail-item5-list2\">\r\n              <img src=\"../assets/箭头 right.png\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"detail-center-item6\">推荐套餐</div>\r\n          <div class=\"detail-center-item8\">\r\n            <div class=\"detail-item8-list box\">\r\n              <img :src=\"item.img\" />\r\n              <p class=\"detail-item8-list-p1\">{{item.title6}}</p>\r\n              <p class=\"detail-item8-list-p2\" ref=\"price1\">￥{{item.price1}}</p>\r\n            </div>\r\n            <div class=\"detail-item8-listt1 boxx\">+</div>\r\n            <div class=\"detail-item8-list\">\r\n              <img :src=\"item.img2\" class=\"item8-img\" />\r\n              <p class=\"detail-item8-list-p1\">{{item.title7}}</p>\r\n              <p class=\"detail-item8-list-p2\">￥{{item.price2}}</p>\r\n              <img src=\"../assets/huigou.png\" class=\"gouimg\" v-if=\"seen22\" @click=\"gou1click\" />\r\n              <img src=\"../assets/honggou.png\" class=\"gouimg\" v-else @click=\"gou1click\" />\r\n            </div>\r\n            <div class=\"detail-item8-list\">\r\n              <img :src=\"item.img3\" class=\"item8-img\" />\r\n              <p class=\"detail-item8-list-p1\">{{item.title8}}</p>\r\n              <p class=\"detail-item8-list-p2\">￥{{item.price3}}</p>\r\n              <img src=\"../assets/huigou.png\" class=\"gouimg\" v-if=\"seen33\" @click=\"gou2click\" />\r\n              <img src=\"../assets/honggou.png\" class=\"gouimg\" v-else @click=\"gou2click\" />\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"datail-center-item9\">\r\n          总价：\r\n          <span class=\"datail-item9-list\">￥{{prices}}</span>\r\n        </div>\r\n        <div class=\"datail-center-item10\" @click=\"mealclick\">\r\n          购买此套餐(\r\n          <span>{{isnum}}</span>件商品\r\n          )\r\n        </div>\r\n        <div class=\"datail-center-item11\" ref=\"span1\">\r\n          <div class=\"datail-item11-list1\">用户评价</div>\r\n          <div class=\"datail-item11-list2\">\r\n            <div class=\"item11box\" @click=\"evaluateclick\">\r\n              <span>好评率：99%</span>\r\n            </div>\r\n            <div class=\"item11box\">\r\n              <img src=\"../assets/箭头 right.png\" class=\"rightimg\" />\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"datail-center-item12\">\r\n          <div class=\"datail-item12-list1\">\r\n            <div class=\"item12-list1box\">\r\n              <img src=\"../assets/tou01.jpg\" />\r\n            </div>\r\n            <div class=\"item12-list1box2\">\r\n              <p class=\"item12-p1\">东方不败</p>\r\n              <p class=\"item12-p2\">2020-08-05</p>\r\n            </div>\r\n          </div>\r\n          <div class=\"datail-item12-list2\">用了几天了，没有被咬过！好评！！！</div>\r\n          <div class=\"datail-item12-list3\">\r\n            <img src=\"../assets/ping01.jpg\" />\r\n          </div>\r\n        </div>\r\n        <div class=\"datail-center-item13\">\r\n          <div class=\"datail-item13-list1\">\r\n            <span>米粉点评团</span>\r\n            <img src=\"../assets/箭头 right.png\" class=\"rightimg\" />\r\n          </div>\r\n          <div class=\"datail-item13-list\">\r\n            <div class=\"datail-item13-listbox11\" v-for=\"item in list[4]\" :key=\"item.price\">\r\n              <div class=\"datail-item13-listbox1\">\r\n                <img :src=\"item.img\" />\r\n              </div>\r\n              <div class=\"datail-item13-listbox2\">{{item.text}}</div>\r\n              <div class=\"datail-item13-listbox3\">\r\n                <div class=\"datail-listbox3-item listitem03\">\r\n                  <img :src=\"item.tou\" />\r\n                  <span>{{item.name}}</span>\r\n                </div>\r\n                <div class=\"datail-listbox3-item listitem04\">\r\n                  <img src=\"../assets/眼睛3.png\" />\r\n                  <span>{{item.price}}</span>\r\n                </div>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <!-- 商品介绍 -->\r\n        <div class=\"datail-center-item14\" ref=\"span2\">商品介绍</div>\r\n        <div class=\"datail-center-item15\" v-for=\"item in list[5]\" :key=\"item.img\">\r\n          <img :src=\"item.img\" />\r\n        </div>\r\n      </div>\r\n    </transition>\r\n\r\n    <!-- 底部 -->\r\n    <div class=\"detail-bottombox\">\r\n      <div class=\"detail-bottombox-item1\">\r\n        <div class=\"detail-item1-list\" @click=\"shouclick\">\r\n          <img src=\"../assets/房子1.png\" alt />\r\n          <p>首页</p>\r\n        </div>\r\n        <div class=\"detail-item1-list bottom-count\" @click=\"gouwuclick\">\r\n          <img src=\"../assets/购物车.png\" alt />\r\n          <p>购物车</p>\r\n          <div class=\"countbox\" v-if=\"count>0\">{{count}}</div>\r\n        </div>\r\n      </div>\r\n      <div class=\"detail-bottombox-item2\" @click=\"popclick(list[list.length-1])\">加入购物车</div>\r\n    </div>\r\n    <!-- 追罩层 -->\r\n    <div class=\"popbox\" :style=\"{display:isdisplay}\">\r\n      <div class=\"popbox1\">\r\n        <img src=\"../assets/对号.png\" />\r\n      </div>\r\n      <div class=\"popbox1\">成功加入购物车！</div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nexport default {\r\n  props:[\"item\"],\r\n  data() {\r\n    return {\r\n      isdisplay: \"none\",\r\n      isnum: 1,\r\n      num22: 0,\r\n      seen22: true,\r\n      num33: 0,\r\n      seen33: true,\r\n      seen11: false,\r\n      isdis: false,\r\n      scrolltop: 0,\r\n      seen: true,\r\n      timer:null,\r\n      swiperOptions: {\r\n        pagination: {\r\n          el: \".swiper-pagination\",\r\n          type: \"fraction\",\r\n          //   clickable: true, // 允许点击小圆点跳转\r\n        },\r\n        autoplay: {\r\n          delay: 3000, //时间\r\n          disableOnInteraction: false, // 手动切换之后继续自动轮播\r\n        },\r\n        loop: true,\r\n      },\r\n      list:[],\r\n      list3:[],\r\n      list33:[],\r\n      list44:[]\r\n    };\r\n  },\r\n  // 计算总价格\r\n  computed:{\r\n     wrap () {\r\n        return this.$store.state.wrap\r\n    },\r\n    count(){\r\n        let numm=0;\r\n        for(let i=0;i<this.wrap.length;i++){\r\n            if(this.wrap[i].checked==true){\r\n                 numm+=this.wrap[i].num\r\n            }\r\n        }\r\n        return numm\r\n    },\r\n  \r\n    prices() {\r\n      if (this.seen22 == false && this.seen33 == true) {\r\n        return parseInt(this.list33.price1) + parseInt(this.list33.price2);\r\n      } else if (this.seen33 == false && this.seen22 == true) {\r\n        return parseInt(this.list33.price1) + parseInt(this.list33.price3);\r\n      } else if (this.seen33 == false && this.seen22 == false) {\r\n        return parseInt(this.list33.price1) + parseInt(this.list33.price2) + parseInt(this.list33.price3);\r\n      } else {\r\n        return parseInt(this.list33.price1);\r\n      }\r\n    },\r\n  },\r\n// axios请求\r\n    created() {\r\n      let url = `http://127.0.0.1:5500/dist/data/DetailsList${this.$route.query.id}.json`;\r\n      let that = this;\r\n      console.log(\"chengg\");\r\n      axios\r\n        .get(url)\r\n        .then(function (response) {\r\n          if (response.data.code == 200) {\r\n            console.log(response);\r\n            that.list = response.data.list;\r\n            that.list3 = response.data.list[3];\r\n            that.list33=that.list3[0];\r\n            that.list44=that.list[that.list.length-2]\r\n            console.log(that.list44);\r\n          }\r\n        })\r\n        .catch(function (error) {\r\n          console.log(error);\r\n        });\r\n    },\r\n  methods: {\r\n    //购买套餐\r\n    mealclick(){\r\n      for(let i=0;i<this.list44.length;i++){\r\n        if(this.list44[i].checked==true){\r\n          this.$store.commit(\"adddetail\",this.list44[i])\r\n        }\r\n      }\r\n      this.$router.push({\r\n        path:\"/cart\"\r\n      })\r\n    },\r\n    // 跳转好评\r\n    evaluateclick() {\r\n      this.$router.push({\r\n        path: \"/evaluate\",\r\n      });\r\n    },\r\n    // 跳转\r\n    clickone() {\r\n      let that = this;\r\n      let timer = setInterval(function () {\r\n        that.$refs.scrollbox.scrollTop -= 120;\r\n        if (that.$refs.scrollbox.scrollTop <= 0) {\r\n          clearInterval(timer);\r\n        }\r\n      }, 50);\r\n    },\r\n    clicktwo(n) {\r\n      let that = this;\r\n        clearInterval(that.timer)\r\n        that.timer = setInterval(function () {\r\n          let y=that.$refs.scrollbox.scrollTop;\r\n          let nn=n-50;\r\n          let x = Math.abs((y-nn)/10);\r\n          if(y==nn){\r\n            clearInterval(that.timer);\r\n          }\r\n          if (y < nn) {\r\n            that.$refs.scrollbox.scrollTop += Math.ceil(x);\r\n          } else if(y > nn) {\r\n            that.$refs.scrollbox.scrollTop -=Math.ceil(x);\r\n          }         \r\n        }, 1);     \r\n    },\r\n    \r\n    // 追罩层添加购物车\r\n    popclick(item) {\r\n      this.isdisplay = \"block\";\r\n      let that = this;\r\n      this.$store.commit(\"adddetail\",item)\r\n      console.log(item)\r\n      console.log(this.wrap)\r\n\r\n      setTimeout(function () {\r\n        that.isdisplay = \"none\";\r\n      }, 3000);\r\n    },\r\n    // 推荐商品下面的勾\r\n    gou1click() {\r\n      this.num22++;\r\n      if (this.num22 % 2 == 0) {\r\n        this.seen22 = true;\r\n        this.isnum--;\r\n        this.list44[1].checked=false\r\n      } else {\r\n        this.seen22 = false;\r\n        this.isnum++;\r\n        this.list44[1].checked=true\r\n      }\r\n    },\r\n    gou2click() {\r\n      this.num33++;\r\n      if (this.num33 % 2 == 0) {\r\n        this.seen33 = true;\r\n        this.isnum--;\r\n        this.list44[2].checked=false\r\n      } else {\r\n        this.seen33 = false;\r\n        this.isnum++;\r\n        this.list44[2].checked=true\r\n      }\r\n    },\r\n    // 返回方法\r\n    goclick() {\r\n      this.$router.go(-1);\r\n    },\r\n    shouclick(){\r\n      this.$router.push({\r\n        path:\"/\"\r\n      })\r\n    },\r\n    gouwuclick(){\r\n      this.$router.push({\r\n        path:\"/cart\"\r\n      })\r\n    },\r\n    // scroll事件\r\n    myscroll(event) {\r\n      this.scrolltop = event.target.scrollTop;\r\n      let y = this.scrolltop / 400;\r\n      this.$refs.topbox.style = `background:rgba(255, 255, 255,${y})`; //顶部div的背景透明度\r\n      if (this.scrolltop > 200) {\r\n        this.seen = false;\r\n      } else {\r\n        this.seen = true;\r\n      }\r\n      if (this.scrolltop >= 200) {\r\n        this.isdis = true;\r\n      } else {\r\n        this.isdis = false;\r\n      }\r\n      if (this.scrolltop >= 1569) {\r\n        this.seen11 = true;\r\n      } else {\r\n        this.seen11 = false;\r\n      }\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.bigbox {\r\n  display: flex;\r\n  flex-direction: column;\r\n  justify-content: space-between;\r\n  height: 100%;\r\n}\r\n.detail-topbox {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 10px 15px;\r\n  position: fixed;\r\n  width: 100%;\r\n  top: 0;\r\n  left: 0;\r\n  z-index: 5;\r\n  height: 60px;\r\n}\r\n.detail-topbox-item img {\r\n  width: 32px;\r\n  height: 32px;\r\n}\r\n.posibox {\r\n  position: fixed;\r\n  top: 52px;\r\n  left: 0;\r\n  z-index: 5;\r\n  width: 100%;\r\n  text-align: center;\r\n  background-color: white;\r\n}\r\n.detail-topbox-item {\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n.topelse img {\r\n  width: 22px;\r\n  height: 22p